<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.bisnisumkm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".BisnisUmkmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:preserveLegacyExternalStorage="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BisnisUMKM"
        android:usesCleartextTraffic="true"
        tools:targetApi="r">
        <activity
            android:name=".testnActivity"
            android:exported="false"
            android:label="@string/title_activity_testn"
            android:theme="@style/Theme.BisnisUMKM.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".PenitipanActivity"
            android:exported="false"
            android:label="@string/title_activity_penitipan"
            android:theme="@style/Theme.BisnisUMKM.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".TestActivity"
            android:exported="false"
            android:label="@string/title_activity_test"
            android:theme="@style/Theme.BisnisUMKM.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".presentation.home.produsen.detail.DetailPenjualOnProdusenActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.home.penjual.detail.DetailProdusenPenjualActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.home.produsen.activity.HomeProdusenActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.home.admin.activity.AdminHomeActivity"
            android:exported="false"
            android:label="@string/title_activity_admin_home" />
        <activity
            android:name=".presentation.register.activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.home.penjual.activity.PenjualHomeActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.login.penjual.activity.PenjualActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.login.login.activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.welcome.WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>